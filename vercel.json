{
  "builds": [
    {
      "src": "port/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/services", "destination": "/" },
    { "source": "/contact", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ]
}
